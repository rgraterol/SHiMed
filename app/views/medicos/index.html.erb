<h1>Directorio MÃ©dico</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Cedula</th>
      <th>Direccion</th>
      <th>Telefono casa</th>
      <th>Telefono celular</th>
      <th>Telefono oficina</th>
      <th>Sexo</th>
      <th>Fecha nacimiento</th>
      <th>Cmv</th>
      <th>Cv</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medicos.each do |medico| %>
      <tr>
        <td><%= medico.email %></td>
        <td><%= medico.nombre %></td>
        <td><%= medico.apellido %></td>
        <td><%= medico.cedula %></td>
        <td><%= medico.direccion %></td>
        <td><%= medico.telefono_casa %></td>
        <td><%= medico.telefono_celular %></td>
        <td><%= medico.telefono_oficina %></td>
        <td><%= medico.sexo %></td>
        <td><%= medico.fecha_nacimiento %></td>
        <td><%= medico.cmv %></td>
        <td><%= medico.cv %></td>
        <td><%= medico.url %></td>
        <td><%= link_to 'Show', medico %></td>
        <td><%= link_to 'Edit', edit_medico_path(medico) %></td>
        <td><%= link_to 'Destroy', medico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

